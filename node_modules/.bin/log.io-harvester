#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../log.io/bin/log.io-harvester" "$@"
  ret=$?
else 
  node  "$basedir/../log.io/bin/log.io-harvester" "$@"
  ret=$?
fi
exit $ret
